{
  "name": "basic-calculator",
  "version": "1.0.0",
  "description": "Basic Calculator using Socket.io",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=true npm install --prefix client && npm run serve --prefix client"
  },
  "author": "Jignesh Jain",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.9",
    "nodemon": "^2.0.6",
    "serve-static": "^1.14.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^3.0.4"
  }
}
